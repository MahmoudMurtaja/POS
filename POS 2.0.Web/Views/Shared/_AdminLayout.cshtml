    <script>

        toastr.options = {
            "closeButton": false,
            "debug": false,
            "newestOnTop": false,
            "progressBar": false,
            "positionClass": "toast-bottom-right",
            "preventDuplicates": false,
            "onclick": null,
            "showDuration": "300",
            "hideDuration": "1000",
            "timeOut": "5000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
        };
    </script>
    @if (TempData["msg"] != null)
    {
        var msg = TempData["msg"].ToString().Substring(2);
        if (TempData["msg"].ToString().StartsWith("s:"))
        {
            <script>
                toastr.success("Message", "@msg");
            </script>
        }
        else if (TempData["msg"].ToString().StartsWith("w:"))
        {
            <script>
                toastr.warning("Message", "@msg");
            </script>
        }
        else if (TempData["msg"].ToString().StartsWith("i:"))
        {
            <script>
                toastr.info("Message", "@msg");
            </script>
        }
        else if (TempData["msg"].ToString().StartsWith("e:"))
        {
            <script>
                toastr.error("Message", "@msg");
            </script>
        }
    }
